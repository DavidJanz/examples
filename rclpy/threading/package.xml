<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format2.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="2">
  <name>examples_rclpy_threading</name>
  <version>0.0.0</version>
  <description>Package with examples of using threading with rclpy.</description>
  <maintainer email="dhood@osrfoundation.org">D. Hood</maintainer>
  <license>Apache License 2.0</license>

  <!-- note that this is only a symbolic dependency on matplotlib:
  it won't actually be resolved by rosdep as the rosdep key does not currently exist -->
  <exec_depend>python3-matplotlib</exec_depend>
  <exec_depend>rclpy</exec_depend>
  <exec_depend>std_msgs</exec_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
